<!-- twitpic.com - Looks for http://twitpic.com/photos/USER -->

<module>
    <site>
        <name>twitpic.com (Username)</name>
        <url><![CDATA[http://twitpic.com/photos/<ACCOUNT>]]></url>
        <method>GET</method>
        <postParameters></postParameters>
        <headers>Accept-Language: en-gb</headers>
        <requestCookie>False</requestCookie>
        <requestCSRF>False</requestCSRF>
        <successmatch>Photos and Videos by</successmatch>
        <negativematch>User could not be found</negativematch>
        <date>10/12/2012</date>
        <version>1</version>
        <author>Jaime Filson aka WiK</author>
        <category>media</category>
    </site>
</module>