<!-- flickr.com - Looks for www.flickr.com/photos/USER -->

<module>
    <site>
        <name>flickr.com (Username)</name>
        <url><![CDATA[http://www.flickr.com/photos/<ACCOUNT>]]></url>
        <method>GET</method>
        <postParameters></postParameters>
        <headers>Accept-Language: en-gb</headers>
        <requestCookie>False</requestCookie>
        <requestCSRF>False</requestCSRF>
        <successmatch>Subscribe to</successmatch>
        <negativematch>Oops! Looks like you followed a bad link.</negativematch>
        <date>10/12/2012</date>
        <version>1</version>
        <author>Jaime Filson aka WiK</author>
        <category>media</category>
    </site>
</module>