<!-- vimeo.com - Looks for http://vimeo.com/USER -->

<module>
    <site>
        <name>vimeo.com (Username)</name>
        <url><![CDATA[http://vimeo.com/<ACCOUNT>]]></url>
        <method>GET</method>
        <postParameters></postParameters>
        <headers>Accept-Language: en-gb</headers>
        <requestCookie>False</requestCookie>
        <requestCSRF>False</requestCSRF>
        <successmatch>Joined</successmatch>
        <negativematch>Page Not Found</negativematch>
        <date>10/12/2012</date>
        <version>1</version>
        <author>Jaime Filson aka WiK</author>
        <category>media</category>
    </site>
</module>