<!-- hulu.com - Looks for http://www.hulu.com/profiles/USER -->

<module>
    <site>
        <name>hulu.com (Username)</name>
        <url><![CDATA[http://www.hulu.com/profiles/<ACCOUNT>]]></url>
        <method>GET</method>
        <postParameters></postParameters>
        <headers>Accept-Language: en-gb</headers>
        <requestCookie>False</requestCookie>
        <requestCSRF>False</requestCSRF>
        <successmatch>Joined On:</successmatch>
        <negativematch>The page you were lookin for doesn't exist (404 error)</negativematch>
        <date>10/12/2012</date>
        <version>1</version>
        <author>Jaime Filson aka WiK</author>
        <category>media</category>
    </site>
</module>