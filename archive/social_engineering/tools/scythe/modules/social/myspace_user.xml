<!-- myspace.com - Looks for www.myspace.com/USER -->

<module>
    <site>
        <name>myspace.com (Username)</name>
        <url><![CDATA[http://www.myspace.com/<ACCOUNT>]]></url>
        <method>GET</method>
        <postParameters></postParameters>
        <headers>Accept-Language: en-gb</headers>
        <requestCookie>False</requestCookie>
        <requestCSRF>False</requestCSRF>
        <successmatch>All Rights Reserved</successmatch>
        <negativematch>Your home page is undergoing routine maintenance</negativematch>
        <date>10/12/2012</date>
        <version>1</version>
        <author>Jaime Filson aka WiK</author>
        <category>social</category>
    </site>
</module>