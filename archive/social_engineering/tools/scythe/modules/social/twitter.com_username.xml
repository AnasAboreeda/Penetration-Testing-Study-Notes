<!-- Twitter.com - Forgot password function -->
<module>
    <site>
        <name>Twitter.com (username)</name>
        <url><![CDATA[http://twitter.com/account/valid_username?username=<ACCOUNT>&value=<ACCOUNT>]]></url>
        <method>GET</method>
        <postParameters></postParameters>
        <headers>Accept-Language: en-gb</headers>
        <requestCookie>False</requestCookie>
        <requestCSRF>False</requestCSRF>
        <successmatch>Looks good!</successmatch>
        <negativematch>Username is invalid</negativematch>
        <date>13/09/2012</date>
        <version>2</version>
        <author>CJR</author>
        <category>social</category>
    </site>
</module>