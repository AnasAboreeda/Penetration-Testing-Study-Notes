<!-- bitly.com - Looks for http://bit.ly/u/USER -->

<module>
    <site>
        <name>bitly.com (Username)</name>
        <url><![CDATA[http://bit.ly/u/<ACCOUNT>]]></url>
        <method>GET</method>
        <postParameters></postParameters>
        <headers>Accept-Language: en-gb</headers>
        <requestCookie>False</requestCookie>
        <requestCSRF>False</requestCSRF>
        <successmatch>BITMARKS</successmatch>
        <negativematch>Uh oh, bitly couldn't find a link for the bitly URL you clicked.</negativematch>
        <date>10/13/2012</date>
        <version>1</version>
        <author>WiK</author>
        <category>social</category>
    </site>
</module>