<!-- livejournal.com - Looks for http://USER.livejournal.com/ -->

<module>
    <site>
        <name>livejournal.com (Username)</name>
        <url><![CDATA[http://<ACCOUNT>.livejournal.com]]></url>
        <method>GET</method>
        <postParameters></postParameters>
        <headers>Accept-Language: en-gb</headers>
        <requestCookie>False</requestCookie>
        <requestCSRF>False</requestCSRF>
        <successmatch>most recent journal entries recorded</successmatch>
        <negativematch>Unknown Journal</negativematch>
        <date>10/12/2012</date>
        <version>1</version>
        <author>Jaime Filson aka WiK</author>
        <category>blogs</category>
    </site>
</module>